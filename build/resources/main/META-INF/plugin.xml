<idea-plugin>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="192.5728" until-build="192.*"/>
  <id>idea-plugin.kotlin-process</id>
  <name>KotlinProcessPlugin</name>
  <vendor url="http://www.jetbrains.com">JetBrains</vendor>
  <depends>com.github.jk1.ytplugin</depends>
  <depends>Git4Idea</depends>
  <depends>org.jetbrains.plugins.github</depends>
  <depends>com.intellij.tasks</depends>
  <actions>
    <action id="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunStopAction" class="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunStopAction" text="Stop Remote Run"/>
    <action id="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunStartAction" class="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunStartAction" text="Start Remote Run"/>
    <action id="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunSettingsAction" class="org.jetbrains.kotlin.process.plugin.actions.rr.RemoteRunSettingsAction" text="Settings"/>
    <action id="org.jetbrains.kotlin.process.plugin.actions.merge.MergeAction" class="org.jetbrains.kotlin.process.plugin.actions.merge.MergeAction" text="Merge to Master"/>
    <action id="org.jetbrains.kotlin.process.plugin.actions.issue.TicketAction" class="org.jetbrains.kotlin.process.plugin.actions.issue.TicketAction" text="Ticket"/>
    <action id="org.jetbrains.kotlin.process.plugin.actions.pullRequest.PullRequestAction" class="org.jetbrains.kotlin.process.plugin.actions.pullRequest.PullRequestAction" text="Pull Request" description="Create pull request from current branch"/>
  </actions>
  <extensions defaultExtensionNs="com.intellij">
    <toolWindow id="Kotlin Process" secondary="true" anchor="bottom" factoryClass="org.jetbrains.kotlin.process.plugin.KotlinProcessToolWindow" icon="/icon/kotlin.svg"/>
  </extensions>
</idea-plugin>
